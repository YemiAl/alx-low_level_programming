#include "lists.h"

size_t listint_len(const listint_t *h)
{
	size_t i = 0;
	const listint_t *temp;

	temp = h;

	while (temp)
	{
		i++;
		temp = temp->next;
	}

	return (i);
}


	
    size_t i = 0;
    const listint_t *temp;
    temp = h;

    while (temp)
    {
        i++;
        temp = temp->next;
    }
    return (i);
}
